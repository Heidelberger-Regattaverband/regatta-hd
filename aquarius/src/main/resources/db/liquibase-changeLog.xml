<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xmlns:pro="http://www.liquibase.org/xml/ns/pro"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
  <changeSet author="markus@ofterdinger.de" id="1">
    <preConditions onFail="MARK_RAN">
      <not>
        <tableExists tableName="AgeClassExt" />
      </not>
    </preConditions>
    <createTable tableName="AgeClassExt">
      <column name="id" type="INT">
        <constraints nullable="false" primaryKey="true" primaryKeyName="PK_AgeClassExt"
          referencedTableName="AgeClass" referencedColumnNames="AgeClass_ID"
          foreignKeyName="FK_AgeClass" />
      </column>
      <column name="distance" type="INT">
        <constraints nullable="false" />
      </column>
    </createTable>
  </changeSet>

  <changeSet author="liquibase-docs" id="2" runOnChange="true">
    <loadUpdateData encoding="UTF-8" file="AgeClassExt.csv" quotchar="&quot;"
      relativeToChangelogFile="true" separator="," tableName="AgeClassExt" primaryKey="id">
      <column header="id" name="id" type="NUMERIC" />
      <column header="distance" name="distance" type="NUMERIC" />
      <column header="comment" type="SKIP" />
    </loadUpdateData>
  </changeSet>

</databaseChangeLog>
